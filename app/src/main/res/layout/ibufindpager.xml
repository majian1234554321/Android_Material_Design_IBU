<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#11000000"
    android:orientation="vertical">

    <com.majian.apple.ibu.menu.item_home.view.MyRelativeLayout
        android:id="@+id/share"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        android:layout_marginTop="16dp"></com.majian.apple.ibu.menu.item_home.view.MyRelativeLayout>

    <com.majian.apple.ibu.menu.item_home.view.MyRelativeLayout
        android:id="@+id/message"
        android:layout_width="match_parent"
        android:layout_marginBottom="16dp"
        android:layout_height="wrap_content"></com.majian.apple.ibu.menu.item_home.view.MyRelativeLayout>

    <com.majian.apple.ibu.menu.item_home.view.MyRelativeLayout
        android:id="@+id/scan"
        android:layout_width="match_parent"

        android:layout_height="wrap_content"></com.majian.apple.ibu.menu.item_home.view.MyRelativeLayout>

    <com.majian.apple.ibu.menu.item_home.view.MyRelativeLayout
        android:id="@+id/aliance"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></com.majian.apple.ibu.menu.item_home.view.MyRelativeLayout>

    <com.majian.apple.ibu.menu.item_home.view.MyRelativeLayout
        android:id="@+id/nav"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></com.majian.apple.ibu.menu.item_home.view.MyRelativeLayout>


</LinearLayout>